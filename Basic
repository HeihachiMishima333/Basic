#include <stdio.h>

void main() {
    int x, y, a = 2, b = 1, t;
    scanf("%d %d", & x, & y);

    t = y;
    while (b <= x)

    {
      printf("* ");
      b++;
    }
    printf("\n");

    for (b = 1, a = 1; a <= t - 2; b++) {
      if (b == 1)
        printf("*");
      else if (b == x)
        printf(" *");

      else
        printf("  ");

      if (b > x) {
        a++;
        b = 0;
        printf("\n");

      }

    }

    a = 1;

    while (a <= x) {
      printf("* ");
      a++;

    }

    printf("\n%d %d", x * y, x + y);

  }

  -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --#include <stdio.h>

#include <math.h>

void main() {
    int a, b, count = 0, i, sum = 0, p = 0;
    printf("Enter a binary number:");
    scanf("%d", & a); {
      b = a;
      while (a != 0) {
        a = a / 10;
        count++;

      }
    }
    while (b != 0) {
      if (b % 10 == 1)
        sum += pow(2, p);
      p++;
      b = b / 10;
    }
    printf("The decimal value is=%d", sum);

  }

  -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- - #include <stdio.h>

  void main() {
    int a, c = 1, sum = 0;
    printf("Enter a decimal number:");
    scanf("%d", & a);
    while (a != 0) {
      if (a % 2 == 1)
        sum += c;

      a /= 2;

      c *= 10;

    }
    printf("The binary value=%d", sum);
  }

  -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -

  #include <stdio.h>

  #include <math.h>

  void main() {
    float a, b, c, result_1, result_2;
    printf("Enter a square equation:");
    scanf("%f %f %f", & a, & b, & c);

    result_1 = (-b + sqrt(b * b - 4 * a * c)) / (2 * a);
    result_2 = (-b - sqrt(b * b - 4 * a * c)) / (2 * a);
    printf("Answers=%f\n%f", result_1, result_2);

  }

  -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -

  #include <stdio.h>

  #include<math.h>

  #include <stdlib.h>

  #include<time.h>

  void main() {
    int a, b, i;
    printf("You have 3 attempts to guess which number the rolled dice will give you from 1 to 6\n");

    srand(time(NULL));
    for (i = 1; i <= 3; i++) {
      scanf("%d", & a);

      b = rand() % 6 + 1;
      if (a == b) {
        printf("Corect!The random number is %d\n", b);
        break;
      } else if (i <= 2)
        printf("Wrong!The random number is %d Try again\n", b);
      else
        printf("Wrong!The random number is %d \n", b);

    }
    if (i == 1)
      printf("Congratulations!, you have guessed correctly at your first attempt.");
    else if (i == 2)
      printf("Congratulations!, you have guessed correctly at your second attempt.");
    else if (i == 3)
      printf("Congratulations, you have guessed correctly at your last attempt");
    else

      printf("Your 3 attempts have failed");

  }

  -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --

#include <stdio.h>

void main() {
    int a, i, k, e = 1;
    scanf("%d", & a);
    printf("2 3 ");
    for (i = 4; i <= a; i++) {

      for (k = 2; k < i; k++)

        if (i % k == 0)
          e = 0;

      if (e == 1)
        printf("%d ", i);
      e = 1;
    }

  }

  -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --#include <stdio.h>

void main() {
    char calculator[1000], c;
    int i = 0, q, flag = 1, k, t, f;
    float count, a[1000], result = 0, x = 0, y = 0;

    scanf("%s", calculator);

    while (calculator[i] != '=') {

      x = 0;
      count = 0.1;
      f = 0;

      if (calculator[i] >= '1' && calculator[i] <= '9') {
        while (calculator[i] != '*' && calculator[i] != '-' && calculator[i] != '+' && calculator[i] != '/' && calculator[i] != '=') {
          count *= 10;

          a[i] = (float)(calculator[i]);
          a[i] -= 48.0;

          i++;
          f = 1;

        }
        if (f == 1) {
          for (k = 0; k < i; k++) {

            result += a[k] * count;

            count /= 10;

          }
        }

      } else if (calculator[i] == '-') {

        i++;
        t = i;
        while (calculator[i] != '*' && calculator[i] != '-' && calculator[i] != '+' && calculator[i] != '/' && calculator[i] != '=') {

          a[i] = (float)(calculator[i]);
          a[i] -= 48.0;
          count *= 10;
          f = 2;
          i++;
        }
        if (f == 2) {
          for (t; t < i; t++) {

            x += a[t] * count;

            count /= 10;

          }

        }
        if (calculator[i] != '*' && calculator[i] != '/') {

          result -= x;
        } else

        {
          if (calculator[i] == '*')
            c = '*';
          else
            c = '/';
          while (calculator[i] == '*' || calculator[i] == '/') {
            i++;
            y = 0;
            count = 0.1;
            t = i;
            while (calculator[i] != '*' && calculator[i] != '-' && calculator[i] != '+' && calculator[i] != '/' && calculator[i] != '=') {

              a[i] = (float)(calculator[i]);
              a[i] -= 48.0;
              count *= 10;

              f = 2;
              i++;
            }
            if (f == 2) {

              for (t; t < i; t++) {

                y += a[t] * count;

                count /= 10;

              }
              if (c == '*')
                x *= y;
              else
                x /= y;

            }

          }
          if (x > 0)
            result -= x;
          else
            result += x;

        }

      } else if (calculator[i] == '+') {

        i++;
        t = i;
        while (calculator[i] != '*' && calculator[i] != '-' && calculator[i] != '+' && calculator[i] != '/' && calculator[i] != '=') {

          a[i] = (float)(calculator[i]);
          a[i] -= 48.0;
          count *= 10;
          f = 2;
          i++;
        }
        if (f == 2) {
          for (t; t < i; t++) {

            x += a[t] * count;

            count /= 10;

          }

        }
        if (calculator[i] != '*' && calculator[i] != '/') {

          result += x;
        } else

        {
          if (calculator[i] == '*')
            c = '*';
          else
            c = '/';
          while (calculator[i] == '*' || calculator[i] == '/') {
            i++;
            y = 0;
            count = 0.1;
            t = i;
            while (calculator[i] != '*' && calculator[i] != '-' && calculator[i] != '+' && calculator[i] != '/' && calculator[i] != '=') {

              a[i] = (float)(calculator[i]);
              a[i] -= 48.0;
              count *= 10;

              f = 2;
              i++;
            }
            if (f == 2) {

              for (t; t < i; t++) {

                y += a[t] * count;

                count /= 10;

              }
              if (c == '*')
                x *= y;
              else
                x /= y;
            }

            result += x;

          }
        }
      } else if (calculator[i] == '*') {
        y = 0;
        i++;
        t = i;
        while (calculator[i] != '*' && calculator[i] != '-' && calculator[i] != '+' && calculator[i] != '/' && calculator[i] != '=') {

          a[i] = (float)(calculator[i]);
          a[i] -= 48.0;
          count *= 10;
          f = 2;
          i++;
        }
        if (f == 2) {
          for (t; t < i; t++) {

            x += a[t] * count;

            count /= 10;

          }

        }
        result *= x;

      } else if (calculator[i] == '/') {

        i++;
        t = i;
        while (calculator[i] != '*' && calculator[i] != '-' && calculator[i] != '+' && calculator[i] != '/' && calculator[i] != '=') {

          a[i] = (float)(calculator[i]);
          a[i] -= 48.0;
          count *= 10;
          f = 2;
          i++;
        }
        if (f == 2) {
          for (t; t < i; t++) {

            x += a[t] * count;

            count /= 10;

          }
          result /= x;

        }

      }
    }

    printf("result=%f", result);
  }

  -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --

#include <stdio.h>

void main() {
    char a[100];
    float b[100], sum = 0, k = 1, sum2 = 0;
    int i = 0, s = 1, t = 1, series, x, minus = 0, mishima = 0, j;
    scanf("%s", a);
    scanf("%d", & series);
    if (a[i] == '-') {
      i++;
      minus = 1;

    }
    while (a[i] != 'N') {
      if (a[i] != '/') {
        mishima = 1;
        break;
      }
      b[i] = (float) a[i] - 48.0;
      s *= t;
      t *= 10;
      i++;

    }
    if (minus == 1)
      i = 1;
    else
      i = 0;
    t /= 10;
    while (t != 0) {
      b[i] *= t;

      t /= 10;
      sum += b[i];
      i++;

    }
    if (mishima == 1) {
      i--;
      j = i;
      t = 1;
      s = 1;
      while (a[i] != 'N') {
        b[i] = (float) a[i] - 48.0;
        s *= t;
        t *= 10;
        i++;
      }
      i = j;
      t /= 10;
      while (t != 0) {
        b[i] *= t;

        t /= 10;
        sum2 += b[i];
        i++;
      }
    }
    if (minus == 1)
      sum2 *= -1;

    for (x = 1; x <= series; x++) {
      printf("%f ", k * sum);
      k++;

    }
  }
  -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- - #include <stdio.h>

  #include <math.h>

  void main() {
    char a[100];
    float b[100], c[100], sum = 0, k = 1, sum2 = 0, burger[100], mult1 = 1, mult2 = 1, goal = 0, king = 0, hezka[100];
    int i = 0, s = 1, t = 1, series, x = 0, minus = 0, mishima = 0, j, h, o, m = 0;
    scanf("%s", a);
    scanf("%d", & series);

    if (a[i] == '-') {
      i++;
      minus = 1;

    }
    while (a[i] != 'N' && a[i] != '/') {

      b[i] = (float) a[i] - 48.0;
      s *= t;
      t *= 10;
      i++;
      m = i;

    }
    if (a[i] == '/') {
      x = 1;
      h = 0;
      o = 1;
      s = 1;
      i++;
      while (a[i] != 'N') {

        c[h] = (float) a[i] - 48.0;
        o *= s;
        s *= 10;
        i++;
        h++;
        m = i;

      }

    }

    if (minus == 1)
      i = 1;
    else
      i = 0;
    t /= 10;
    while (t != 0) {
      b[i] *= t;

      t /= 10;
      sum += b[i];
      i++;

    }
    if (x == 1) {

      for (i = 0; i <= h - 1; i++) {
        c[i] *= o;
        sum2 += c[i];
        o /= 10;

      }

    }
    if (minus == 1)
      sum *= -1;
    if (x == 1) {

      sum /= sum2;
    }

    if (a[m + 1] == '^') {
      king = 1;
      m += 2;;
      i = 0;
      mult1 = 0.1;
      while (a[m] != '=' && a[m] != '+' && a[m] != '-') {

        burger[i] = (float) a[m] - 48.0;

        mult1 *= 10;
        m++;
        i++;

      }
      i = 0;

      while (mult1 >= 1) {
        burger[i] *= mult1;

        goal += burger[i];
        mult1 /= 10;
        i++;

      }

    }
    if (king == 1) {
      for (x = 1; x <= series; x++)

        hezka[x] = pow(x, goal);

      for (x = 1; x <= series; x++) {
        printf("%f\n", sum * hezka[x]);

      }

    } else
      for (x = 1; x <= series; x++) {
        {
          printf("\n%f\n", k * sum);
          k++;

        }
      }
  }
